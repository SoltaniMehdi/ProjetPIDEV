<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AfficherLivraisonController">
    <children>
        <TableView fx:id="livraisonTable" layoutX="31.0" layoutY="26.0" prefHeight="324.0" prefWidth="538.0">
            <columns>
                <TableColumn fx:id="idLivraisonColumn" prefWidth="75.0" text="ID Livraison" />
                <TableColumn fx:id="idCommandeColumn" prefWidth="85.60003662109375" text="ID Commande" />
                <TableColumn fx:id="idClientColumn" prefWidth="64.79998779296875" text="ID Client" />
                <TableColumn fx:id="dateLivraisonColumn" prefWidth="150.0" text="Date Livraison" />
                <TableColumn fx:id="adresseColumn" prefWidth="150.0" text="Adresse" />
                <TableColumn fx:id="prixColumn" prefWidth="75.0" text="Prix" />
            </columns>
        </TableView>
        <Button fx:id="supprimerButton" layoutX="320.0" layoutY="355.0" mnemonicParsing="false" onAction="#supprimer" text="Supprimer">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
      <Button fx:id="modifier" layoutX="193.0" layoutY="355.0" mnemonicParsing="false" onAction="#modifier" text="modifier">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="436.0" layoutY="355.0" mnemonicParsing="false" onAction="#handleAnnulerButton" text="retour">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
