<form method="post" action="{{ path('app_categorie_delete', {'idcateg': categorie.idcateg}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.idcateg) }}">
    <button class="btn">Delete</button>
</form>
