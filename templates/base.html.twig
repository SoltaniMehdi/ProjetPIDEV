    <script>
    function addMeal(nom, prix, description) {
        // AJAX request to add meal to the database
        $.ajax({
            method: 'GET', // Change this to 'POST' if needed
            data: { nom: nom, prix: prix, description: description },
            success: function(response) {
                // Display success message if needed
                console.log(response);
                // Dynamically add the meal to the cart list on the page
                const newItem = document.createElement('li');
                newItem.classList.add('list-group-item', 'd-flex', 'justify-content-between', 'align-items-center');
                newItem.innerHTML = `
                    ${nom}
                    <span class="badge badge-primary badge-pill">1</span>
                `;
                $('#meal-list').append(newItem);
            },
            error: function(xhr, status, error) {
                // Handle error if needed
                console.error(xhr.responseText);
            }
        });
    }


    document.addEventListener("DOMContentLoaded", function() {
    // Find the "Confirmer" button by its id
    var confirmButton = document.getElementById("add-meal-btn");

    // Add a click event listener to the "Confirmer" button
    confirmButton.addEventListener("click", function() {
        // Call a function to handle the button click event
        handleConfirmButtonClick();
    });
});

// Function to handle the button click event
function handleConfirmButtonClick() {
    // Perform any actions you want when the button is clicked
    // For example, you can send an AJAX request to your server to add the command to the database
    // Replace the placeholder URL with the actual endpoint where you handle the request
    var url = "http://example.com/add_command"; // Replace with your actual endpoint
    var xhr = new XMLHttpRequest();
    xhr.open("POST", url);
    xhr.setRequestHeader("Content-Type", "application/json");
    xhr.onreadystatechange = function () {
        if (xhr.readyState === 4 && xhr.status === 200) {
            // Request was successful
            console.log("Commande added successfully");
            // Optionally, you can redirect the user to a confirmation page or perform other actions
        } else if (xhr.readyState === 4 && xhr.status !== 200) {
            // Request failed
            console.error("Failed to add commande");
            // Optionally, you can display an error message to the user
        }
    };
    // Prepare the data to send in the request body (if needed)
    var data = {}; // Add your data here if needed
    xhr.send(JSON.stringify(data));
}


    
</script>


