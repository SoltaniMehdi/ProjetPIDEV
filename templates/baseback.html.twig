<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Welcome to My Website{% endblock %}</title>
    {# Include CSS files #}
    {% block stylesheets %}
         <link rel="stylesheet" href="asset2/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="asset2/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="asset2/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="asset2/vendors/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="asset2/vendors/bootstrap-datepicker/bootstrap-datepicker.min.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="asset2/css/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="asset2/images/favicon.png" />
    {% endblock %}
</head>
<body>
    {# Main content block #}
    {% block body %}{% endblock %}

    {# Include JavaScript files #}
    {% block javascripts %}
        <script src="asset2/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="asset2/vendors/chart.js/Chart.min.js"></script>
    <script src="asset2/vendors/jquery-circle-progress/js/circle-progress.min.js"></script>
    <script src="asset2/js/jquery.cookie.js" type="text/javascript"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="asset2/js/off-canvas.js"></script>
    <script src="asset2/js/hoverable-collapse.js"></script>
    <script src="asset2/js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="asset2/js/dashboard.js"></script>
    <!-- End custom js for this page -->
    {% endblock %}
</body>
</html>
